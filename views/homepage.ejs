    <script>
        $('#nav-tab-two').on('click',async ()=>{
            const myElement = $('#addLanding');
            const dataUser = myElement.data('user');
            const headers = {
                'Content-Type': 'application/json'
            }
            await commonAjax(`/v1/sort/popular?dataUser=${dataUser}`,'GET',headers,null)
                .then(res=>{
                    $('#popular-tab').html(res.data)
                })
        })

        $('#nav-tab-three').on('click',async ()=>{
            const myElement = $('#addLanding');
            const dataUser = myElement.data('user');
            const headers ={
                'Content-Type': 'application/json'
            }
            await commonAjax(`/v1/sort/new-added?dataUser=${dataUser}`,'GET',headers,null)
                .then(res=>{
                    $('#new-added-tab').html(res.data)
                })
                .catch(err=>{
                    swal(res.data)
                })
        })
    </script>
    
