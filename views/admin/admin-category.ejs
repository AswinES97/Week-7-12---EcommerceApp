    <section class="content-main" style="background-color: antiquewhite;">
        <div class="content-header">
            <div>
                <h2 class="content-title card-title">Categories </h2>
                <p>Add, edit or delete a category</p>
            </div>
            <div>
                <input type="text" placeholder="Search Categories" class="form-control bg-white">
            </div>
        </div>
        <div >
            <div class="card-body">
                <div class="row">
                    <div class="col-md-3">
                        <form>
                            <div class="mb-4">
                                <label for="Gender" class="form-label">Gender</label>
                                <select name="Gender" class="form-select" id="category">
                                    
                                </select>
                            </div>
                            <div class="mb-4">
                                <label for="Sub-Category" class="form-label">Sub-Category</label>
                                <select name="subCategory" class="form-select" id="sub-category">
                                    
                                </select>
                            </div>
                            <div class="mb-4">
                                <label for="categoryType" class="form-label">Type</label>
                                <input  type="text" placeholder="Type here" class="form-control" name="categoryType" id="categoryType" />
                                <span id="typeError" hidden style="color: red;">Required!</span>
                            </div>
                            
                            <div class="d-grid">
                                <button class="btn btn-primary" onclick="createCategory(event)">Create category</button>
                                <span hidden id="submitError">Not Submitted!</span>
                            </div>
                        </form>
                    </div> 
                    <div class="col-md-9">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th>Gender</th>
                                        <th>Sub-Category</th>
                                        <th>Category-Type</th>
                                        <th>Order</th>
                                        <th class="text-end">Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <%for(let props in data){%>
                                    <tr>
                                        <td><b><%=props%></b></td>
                                        <td>Men clothes</td>
                                        <td>/men</td>
                                        <td>1</td>
